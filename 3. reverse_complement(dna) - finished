def reverse_complement(dna):
    comp = ''
    for symbol in dna:
        if symbol == 'A':
            comp = comp + 'T'
        elif symbol == 'T':
            comp = comp + 'A'
        elif symbol == 'C':
            comp = comp + 'G'
        elif symbol == 'G':
            comp = comp + 'C'
    return comp[::-1]
