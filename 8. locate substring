def locate_substring(dna_snippet,dna):
    list = []
    for char in range(0,len(dna) - len(dna_snippet) + 1):
        present = False
        if dna_snippet[0] == dna[char]:
            for charo in range(0,len(dna_snippet)):
                if dna[char + charo] != dna_snippet[charo]:
                    break
                if charo == len(dna_snippet) - 1:
                    present = True
        if present:
            list.append(char)
    return list
print(locate_substring("TATA","GATATATGCATATACTT"))
